function Tab1Window(e){var t;t=require("ui/handheld/Style4iOS");var o=Ti.UI.createLabel();o.text=e,t.setWindowTitleLabelStyle(o);var i=Ti.UI.createWindow();i.titleControl=o,i.layout="vertical",t.setWindowStyle(i);var a=Ti.UI.createButton();t.setTab1WindowEditButtonStyle(a),i.rightNavButton=a,a.addEventListener("click",function(){alert("edit button click")});var n=Ti.UI.createButton();t.setTab1WindowBackButtonStyle(n),n.addEventListener("click",function(){alert("back button click")}),i.leftNavButton=n;var r=Ti.Media.createVideoPlayer();t.setTab1WindowMovieViewStyle(r),r.url="/media/infomovie.mp4",i.add(r);var c=Ti.UI.createTableView();t.setTab1WindowTableViewStyle(c),i.add(c);var l=Ti.UI.createTableViewSection(),d=Ti.UI.createTableViewSection(),u=[];u.push({title:"Google",url:"http://google.co.jp",icon:"images/more1ic.png"}),u.push({title:"Yahoo",url:"http://yahoo.co.jp",icon:"images/more2ic.png"}),u.push({title:"bing",url:"http://bing.co.jp",icon:"images/more3ic.png"});var b=[];b.push({title:"Versionï¼š"+Ti.App.iOSAPPVERSION,url:"",icon:"images/more4ic.png"}),b.push({title:"Rate this App",url:"http://uchidak.net",icon:"images/more5ic.png"}),b.push({title:"Other Apps",url:"https://uchidak.net",icon:"images/more6ic.png"}),b.push({title:"Powered by UCHIDAK.NET",url:"http://uchidak.net",icon:"images/more7ic.png"}),b.push({title:"Contact",url:"http://uchidak.net",icon:"images/more8ic.png"});for(var s=0;u.length>s;s++){var T=createRow(u[s].title,u[s].icon,u[s].url);l.add(T)}for(var f=0;b.length>f;f++){var T=createRow(b[f].title,b[f].icon,b[f].url);d.add(T)}return c.appendSection(l),c.appendSection(d),i}function createRow(e,t,o){var i;i=require("ui/handheld/Style4iOS");var a=Ti.UI.createTableViewRow({url:o});i.setTab1WindowRowStyle(a),a.addEventListener("click",function(e){e.row.url&&Ti.Platform.openURL(e.row.url)});var n=Ti.UI.createLabel({text:e});i.setTab1WindowRowTitleLabelStyle(n),a.add(n);var r=Ti.UI.createView();i.setTab1WindowRowIconViewStyle(r),a.add(r);var c=Ti.UI.createImageView({image:t});return r.add(c),a}module.exports=Tab1Window;