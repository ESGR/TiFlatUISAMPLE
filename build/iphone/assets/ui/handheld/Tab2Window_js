function Tab2Window(e){var t;t=require("ui/handheld/Style4iOS");var a=Ti.UI.createLabel();a.text=e,t.setWindowTitleLabelStyle(a);var o=Ti.UI.createWindow();o.titleControl=a,t.setWindowStyle(o);var i=Ti.UI.createButton();i.width=44,i.height=44,i.backgroundImage="images/reload-button.png",o.rightNavButton=i,i.addEventListener("click",function(){alert("reload button click")});var n=Ti.UI.createTableView();return t.setTab2TableViewStyle(n),setTableViewData(n),o.add(n),o}function setTableViewData(e){var t=require("ui/handheld/Style4iOS"),a=Ti.UI.createTableViewSection(),o=[];o.push({thumbnail:"images/thumbnail.jpg",title:"ABCDEあいうえおかきくけこさしすせそたちつてと",count:"10000",seconds:"10:00:00"});for(var i=0;50>i;i++){var n=Ti.UI.createTableViewRow({id:i,className:"tomato"}),r=0;r=i%2?0:1,t.setTab2TableViewRowStyle(n,r);var l=Ti.UI.createImageView();t.setTab2RowImageViewStyle(l),l.image=o[0].thumbnail;var c=Ti.UI.createView();t.setTab2RowTitleAreaStyle(c);var d=Ti.UI.createLabel();d.text=o[0].title,t.setTab2RowTitleLabelStyle(d),n.add(d);var u=Ti.UI.createLabel();t.setTab2RowViewCountStyle(u),u.text=o[0].count;var b=Ti.UI.createLabel();t.setTab2RowSecondsLabelStyle(b),b.text=o[0].seconds,n.add(l),n.add(b),n.add(c),c.add(d),n.add(u),a.add(n)}e.appendSection(a)}module.exports=Tab2Window;